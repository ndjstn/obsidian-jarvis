{
  "name": "obsidian-jarvis",
  "version": "0.4.0",
  "description": "PKM AI assistant plugin for Obsidian with slash commands, context menu, RAG search, and local LLM support",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ai",
    "llm",
    "ollama",
    "langchain",
    "jarvis"
  ],
  "author": "uri",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.20.0",
    "obsidian": "latest",
    "tslib": "^2.6.2",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "ollama": "^0.5.11"
  }
}
